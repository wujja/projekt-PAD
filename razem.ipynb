   "source": [
    "import json\n",
    "import pandas as pd\n",
    "whole_data = []\n",
    "table_of_files = ['dataN.json','dataN2.json','dataN3.json', 'dataN4.json']\n",
    "for file in table_of_files:\n",
    "    with open(file) as f:\n",
    "        whole_data += json.load(f)\n",
    "\n",
    "df = pd.DataFrame(whole_data)\n",
    "\n",
    "df.to_csv('data.csv')"
   ]
  }
